                }

                if (TotalHight > $(".quib-container").height()) {
                    Col = Col + 1;
                    leftPos = ($(this).width() + margin_right) * Col;
                    TotalHight = $(this).height() + margin_bottom;
                    topPos = 0;
                }

                //if ((parseInt($('#quibContainer').width() - $('.quib-item').width())) == 0) {
                //    if (Col == 0) {
                //        if (quib.GlobalDeclarations.fnCurrentFlag(false) == 0) {
                //            $(this).attr('CurCol', 'CurCol');
                //        }
                //    }
                //    else {
                //        if (quib.GlobalDeclarations.fnCurrentFlag(false) == 0) {
                //            $(this).removeAttr('CurCol')
                //        }
                //    }

                //    if (Col == -1) {
                //        if (quib.GlobalDeclarations.fnCurrentFlag(false) == 0) {
                //            $(this).attr('PrevCol', 'PrevCol');
                //        }
                //    }
                //    else {
                //        if (quib.GlobalDeclarations.fnCurrentFlag(false) == 0) {
                //            $(this).removeAttr('PrevCol')
                //        }
                //    }

                //} else {

                //    if (Col == 0) {
                //        if (quib.GlobalDeclarations.fnCurrentFlag(false) == 0) {
                //            $(this).attr('PrevCol', 'PrevCol');
                //        }
                //    }
                //    else {
                //        if (quib.GlobalDeclarations.fnCurrentFlag(false) == 0) {
                //            $(this).removeAttr('PrevCol')
                //        }
                //    }
                //    if (Col == 1) {
                //        if (quib.GlobalDeclarations.fnCurrentFlag(false) == 0) {
                //            $(this).attr('CurCol', 'CurCol');
                //        }
                //    }
                //    else {
                //        if (quib.GlobalDeclarations.fnCurrentFlag(false) == 0) {
                //            $(this).removeAttr('CurCol');
                //        }
                //    }
                //}

                $(this).attr('colmValue', Col.toString());
                $(this).css('top', topPos + 'px')
                $(this).css('left', leftPos + 'px')
            });
            
            setTimeout(function () {
                $('.popup_load').css("display", "none");
            }, 500);
               
        };

    </script>
</head>

